{
  "swagger": "2.0",
  "info": {
    "title": "My API Project for a School's bd",
    "description": "Is a RESTful API for my contacts created as part of my class assignment for CSE 341.",
    "version": "1.0.0"
  },
  "host": "cse-341-webservices-personal-project.onrender.com",
  "basePath": "/",
  "schemes": [
    "https"
  ],
  "paths": {
    "/contacts/": {
      "get": {
        "description": "Retrieve all contacts",
        "produces": [
          "application/json"
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      },
      "post": {
        "description": "Create a new contact",
        "parameters": [
          {
            "name": "first_name",
            "in": "body",
            "required": true,
            "schema": {
              "type": "string",
              "example": "John"
            }
          },
          {
            "name": "last_name",
            "in": "body",
            "required": true,
            "schema": {
              "type": "string",
              "example": "Doe"
            }
          },
          {
            "name": "date_of_birth",
            "in": "body",
            "required": true,
            "schema": {
              "type": "string",
              "example": "mm/dd/yyyy"
            }
          },
          {
            "name": "password",
            "in": "body",
            "required": true,
            "schema": {
              "type": "string",
              "example": "at least 6 characters"
            }
          },
          {
            "name": "nationality",
            "in": "body",
            "required": false,
            "schema": {
              "type": "string",
              "example": "country of birth/citizenship"
            }
          },
          {
            "name": "semester",
            "in": "body",
            "required": false,
            "schema": {
              "type": "string",
              "example": "freshman, sophomore, junior, senior"
            }
          }
        ],
        "responses": {
          "201": {
            "description": "Created"
          },
          "500": {
            "description": "Internal Server Error"
          }
        }
      }
    },
    "/contacts/{id}": {
      "get": {
        "description": "Retrieve a single contact by ID",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          },
          "404": {
            "description": "Not Found"
          }
        }
      },
      "put": {
        "description": "Update a contact by ID",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "type": "string"
          },
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "type": "object",
              "properties": {
                "first_name": {
                  "type": "string"
                },
                "last_name": {
                  "type": "string"
                },
                "date_of_birth": {
                  "type": "string"
                },
                "password": {
                  "type": "string"
                },
                "nationality": {
                  "type": "string"
                },
                "semester": {
                  "type": "string"
                },
                "parents": {
                  "type": "object",
                  "properties": {
                    "mother": {
                      "type": "string"
                    },
                    "father": {
                      "type": "string"
                    }
                  }
                },
                "contact_numbers": {
                  "type": "object",
                  "properties": {
                    "phone_num": {
                      "type": "string"
                    },
                    "parents": {
                      "type": "object",
                      "properties": {
                        "mother": {
                          "type": "string"
                        },
                        "father": {
                          "type": "string"
                        }
                      }
                    }
                  }
                },
                "public_profile": {
                  "type": "object",
                  "properties": {
                    "hobbies": {
                      "type": "string"
                    },
                    "plans_after_graduation": {
                      "type": "string"
                    },
                    "major": {
                      "type": "string"
                    },
                    "years_to_graduate": {
                      "type": "string"
                    }
                  }
                }
              },
              "required": ["first_name", "last_name", "date_of_birth", "password", "nationality", "semester"]
            }
          }
        ],
        "responses": {
          "204": {
            "description": "No Content"
          },
          "500": {
            "description": "Internal Server Error"
          }
        }
      },
      "delete": {
        "description": "",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "204": {
            "description": "No Content"
          },
          "500": {
            "description": "Internal Server Error"
          }
        }
      },
      "patch": {
        "description": "Partially update a contact by ID",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "type": "string"
          },
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "type": "object",
              "properties": {
                "first_name": {
                  "type": "string"
                },
                "last_name": {
                  "type": "string"
                },
                "date_of_birth": {
                  "type": "string"
                },
                "password": {
                  "type": "string"
                },
                "nationality": {
                  "type": "string"
                },
                "semester": {
                  "type": "string"
                },
                "parents": {
                  "type": "object",
                  "properties": {
                    "mother": {
                      "type": "string"
                    },
                    "father": {
                      "type": "string"
                    }
                  }
                },
                "contact_numbers": {
                  "type": "object",
                  "properties": {
                    "phone_num": {
                      "type": "string"
                    },
                    "parents": {
                      "type": "object",
                      "properties": {
                        "mother": {
                          "type": "string"
                        },
                        "father": {
                          "type": "string"
                        }
                      }
                    }
                  }
                },
                "public_profile": {
                  "type": "object",
                  "properties": {
                    "hobbies": {
                      "type": "string"
                    },
                    "years_of_service": {
                      "type": "number"
                    },
                    "department": {
                      "type": "string",
                      "enum": ["Administration"]
                    },
                    "years_to_graduate": {
                      "type": "string"
                    },
                    "role": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        ],
        "responses": {
          "204": {
            "description": "No Content"
          },
          "500": {
            "description": "Internal Server Error"
          }
        }
      }
    }
  }
}
